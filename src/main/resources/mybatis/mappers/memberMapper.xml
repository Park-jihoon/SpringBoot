<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.test.boot.mappers.MemberMapper">
    <select id="getByID" resultType="org.test.boot.domain.Member">
        select * from Member where userid = #{userId} and Password = #{password} and State = '사용중'
    </select>
</mapper>